{
    "name": "demeyerthom/ocr-space",
    "description": "A PHP service to connect with the ocr-space API",
    "keywords": [
        "ocr",
        "ocr-space"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "league/tactician": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.23",
        "jms/serializer": "^1.9",
        "symfony/validator": "^3.3",
        "psr/log": "^1.0",
        "league/tactician-logger": "^0.10.0",
        "symfony/expression-language": "^3.3",
        "symfony/options-resolver": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Demeyerthom\\OcrSpace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Demeyerthom\\OcrSpace\\Test\\": "tests/"
        }
    }
}
